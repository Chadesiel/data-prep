{
  "contentTypes": {},
  "actions": {
    "menu:preparations": {
      "id": "menu:preparations",
      "label": "Preparations",
      "icon": "talend-dataprep",
      "payload": {
        "type": "MENU_LINK",
        "cmf": {
          "routerReplace": "/preparations"
        }
      }
    },
    "headerbar:help": {
      "displayMode":"splitDropdown",
      "id":"headerbar:help",
      "name":"Help",
      "href": "https://help.talend.com/search/all?filters=EnrichPlatform%7E%2522Talend+Data+Preparation%2522*EnrichVersion%7E%25222.1%2522&utm_medium=dpdesktop&utm_source=&content-lang=en",
      "target": "_blank",
      "pullRight": true,
      "actionIds":[
        "help:external",
        "help:community",
        "help:tour",
        "help:about:open",
        "help:feedback:open"
      ]
    },
    "help:external": {
      "id": "help:external",
      "label": "Help",
      "href": "https://help.talend.com/search/all?filters=EnrichPlatform%7E%2522Talend+Data+Preparation%2522*EnrichVersion%7E%25222.1%2522&utm_medium=dpdesktop&utm_source=&content-lang=en",
      "target": "_blank"
    },
    "help:community": {
      "id": "help:community",
      "label": "Community",
      "href": "https://community.talend.com/t5/Data-Quality-and-Preparation/bd-p/prepare_govern",
      "target": "_blank"
    },
    "help:tour": {
      "id": "help:tour",
      "label": "Guided tour",
      "actionCreator":"help:tour"
    },
    "help:about:open": {
      "id": "help:about:open",
      "label": "About Data Preparation",
      "actionCreator": "help:about:open"
    },
    "help:feedback:open": {
      "id": "help:feedback:open",
      "label": "Feedback",
      "actionCreator": "help:feedback:open"
    },
    "preparation:add:open": {
      "id": "preparation:add:open",
      "bsStyle": "primary",
      "icon": "talend-plus-circle",
      "label": "Add preparation",
      "actionCreator": "preparation:add:open"
    },
    "folder:add:open": {
      "id": "folder:add:open",
      "icon": "talend-folder",
      "label": "Add folder",
      "actionCreator": "folder:add:open"
    }
  },
  "props": {
    "indexRouteRedirect": {
      "path": "/preparations"
    },
    "Container(HeaderBar)#default": {
      "logo": {
        "id": "talend-logo",
        "label": "Go to home page",
        "payload": {
          "type": "MENU_LINK",
          "cmf": {
            "routerReplace": "/preparations"
          }
        }
      },
      "brand": {
        "label": "Data Preparation",
        "payload": {
          "type": "MENU_LINK",
          "cmf": {
            "routerReplace": "/preparations"
          }
        }
      },
      "search": {
        "icon": {
          "name": "talend-search",
          "title": "Toggle search input",
          "bsStyle": "link",
          "tooltipPlacement": "bottom"
        },
        "id": "headerbar:search",
        "onToggle": "headerbar:search:toggle"
      },
      "help": {
        "actionId": "headerbar:help"
      }
    },
    "Container(SidePanel)#default": {
      "actionIds": [
        "menu:datastores",
        "menu:datasets",
        "menu:preparations"
      ]
    },
    "datastores": {
      "hasTheme": true
    },
    "datasets": {
      "hasTheme": true
    },
    "preparations" : {
      "hasTheme": true,
      "header": { },
      "sidepanel": { },
      "list": {
        "collectionId": "preparations",
        "list": {
          "columns": [
            { "key": "name", "label": "Name" },
            { "key": "author",  "label": "Author" },
            { "key": "datasetName",  "label": "Dataset" },
            { "key": "nbSteps",  "label": "Steps" }
          ],
          "titleProps": {
            "key": "name",
            "iconKey": "icon"
          }
        },
        "toolbar": {
          "filter": {
            "placeholder": "Find a preparation"
          }
        },
        "actions": {
          "title": "preparation:open",
          "left": ["preparation:add:open", "folder:add:open"]
        }
      }
    }
  },
  "routes": {
    "path": "/",
    "component": "App",
    "indexRoute": {
      "component": "Redirect",
      "view": "indexRouteRedirect"
    },
    "childRoutes": [
      {
        "path": "preparations(/:folderId)",
        "component": "HomeListView",
        "view": "preparations",
        "onEnter": "preparation:fetch"
      },
      {
        "path": "datasets",
        "component": "HomeListView",
        "view": "datasets"
      },
      {
        "path": "datastores",
        "component": "HomeListView",
        "view": "datastores"
      }
    ]
  },
  "ref": {
    "SidePanel#default": {},
    "AppHeaderBar#default": {}
  }
}
