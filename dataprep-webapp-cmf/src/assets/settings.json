{
  "contentTypes": {},
  "actions": {
    "menu:preparations": {
      "id": "menu:preparations",
      "label": "Preparations",
      "icon": "talend-dataprep",
      "payload": {
        "type": "MENU_LINK",
        "cmf": {
          "routerReplace": "/preparations"
        }
      }
    },
    "menu:datasets": {
      "id": "menu:datasets",
      "label": "Datasets",
      "icon": "talend-folder",
      "payload": {
        "type": "MENU_LINK",
        "cmf": {
          "routerReplace": "/datasets"
        }
      }
    },
    "menu:datastores": {
      "id": "menu:datastores",
      "label": "Datastores",
      "icon": "talend-datastore",
      "payload": {
        "type": "MENU_LINK",
        "cmf": {
          "routerReplace": "/datastores"
        }
      }
    },
    "headerbar:help": {
      "displayMode":"splitDropdown",
      "id":"headerbar:help",
      "name":"Help",
      "href": "https://help.talend.com/search/all?filters=EnrichPlatform%7E%2522Talend+Data+Preparation%2522*EnrichVersion%7E%25222.1%2522&utm_medium=dpdesktop&utm_source=&content-lang=en",
      "target": "_blank",
      "pullRight": true,
      "actionIds":[
        "help:external",
        "help:community",
        "help:tour",
        "help:about:open",
        "help:feedback:open"
      ]
    },
    "help:external": {
      "id": "help:external",
      "label": "Help",
      "href": "https://help.talend.com/search/all?filters=EnrichPlatform%7E%2522Talend+Data+Preparation%2522*EnrichVersion%7E%25222.1%2522&utm_medium=dpdesktop&utm_source=&content-lang=en",
      "target": "_blank"
    },
    "help:community": {
      "id": "help:community",
      "label": "Community",
      "href": "https://community.talend.com/t5/Data-Quality-and-Preparation/bd-p/prepare_govern",
      "target": "_blank"
    },
    "help:tour": {
      "id": "help:tour",
      "label": "Guided tour",
      "actionCreator":"help:tour"
    },
    "help:about:open": {
      "id": "help:about:open",
      "label": "About Data Preparation",
      "actionCreator": "help:about:open"
    },
    "help:feedback:open": {
      "id": "help:feedback:open",
      "label": "Feedback",
      "actionCreator": "help:feedback:open"
    }
  },
  "views": {
    "indexRouteRedirect": {
      "path": "/preparations"
    },
    "Container(HeaderBar)#default": {
      "logo": {
        "id": "talend-logo",
        "label": "Go to home page",
        "payload": {
          "type": "MENU_LINK",
          "cmf": {
            "routerReplace": "/preparations"
          }
        }
      },
      "brand": {
        "label": "Data Preparation",
        "payload": {
          "type": "MENU_LINK",
          "cmf": {
            "routerReplace": "/preparations"
          }
        }
      },
      "search": {
        "icon": {
          "name": "talend-search",
          "title": "Toggle search input",
          "bsStyle": "link",
          "tooltipPlacement": "bottom"
        },
        "id": "headerbar:search",
        "onToggle": "headerbar:search:toggle"
      },
      "help": {
        "actionId": "headerbar:help"
      }
    },
    "Container(SidePanel)#default": {
      "actionIds": ["menu:preparations", "menu:datasets", "menu:datastores"]
    },
    "preparations" : {
      "didMountActionCreator": "preparation:fetchAll",
      "hasTheme": true,
      "header": { },
      "sidepanel": { },
      "list": {
        "collectionId": "preparations",
        "list": {
          "columns": [
            { "key": "name", "label": "Name" },
            { "key": "author",  "label": "Author" }
          ],
          "titleProps": {
            "key": "name",
            "iconKey": "icon"
          }
        },
        "actions": {
          "title": "preparation:open"
        }
      }











    },
    "datasets": {
      "didMountActionCreator": "dataset:fetchAll",
      "header": {},
      "sidepanel": {},
      "list": {
        "collectionId": "datasets",
        "list": {
          "columns": [
            { "key": "id", "label": "ID" },
            { "key": "label", "label": "Name" },
            { "key": "created",  "label": "Created" },
            { "key": "tags", "label": "Tags" }
          ]
        },
        "toolbar": {
          "filter": {
            "placeholder": "Find a dataset"
          },
          "sort": {
            "options": [
              { "id": "label", "name": "Name" },
              { "id": "created", "name": "Created" }
            ]
          }
        },
        "initialState": {
          "sortOn": "label"
        }
      }
    },
    "datastores": {
      "didMountActionCreator": "datastore:fetchAll",
      "header": {},
      "sidepanel": {},
      "list": {
        "collectionId": "datastores",
        "list": {
          "columns": [
            { "key": "id", "label": "ID" },
            { "key": "label", "label": "Name" },
            { "key": "type",  "label": "Type" },
            { "key": "datasets", "label": "Number of datasets" }
          ]
        },
        "toolbar": {
          "filter": {
            "placeholder": "Find a datastore"
          },
          "sort": {
            "options": [
              { "id": "label", "name": "Name" },
              { "id": "type", "name": "Type" },
              { "id": "datasets", "name": "Number of datasets" }
            ]
          }
        },
        "initialState": {
          "sortOn": "label"
        }
      }
    }
  },
  "routes": {
    "path": "/",
    "component": "App",
    "indexRoute": {
      "component": "Redirect",
      "view": "indexRouteRedirect"
    },
    "childRoutes": [
      {
        "path": "preparations",
        "component": "HomeListView",
        "view": "preparations"
      },
      {
        "path": "datasets",
        "component": "HomeListView",
        "view": "datasets"
      },
      {
        "path": "datastores",
        "component": "HomeListView",
        "view": "datastores"
      }
    ]
  }
}
